<div id="message">
  {{ message }}
</div>
<div id="info">
  <table>
    <tr>
      <th>namespace:</th>
      <td>{{ namespace }}</td>
    </tr>
    <tr>
      <th>pod:</th>
      <td>{{ pod }}</td>
    </tr>
    <tr>
      <th>podIP:</th>
      <td>{{ podIP }}</td>
    </tr>
    <tr>
      <th>node:</th>
      <td>{{ node }}</td>
    </tr>
  </table>
  </br>
  <table>
    <tr>
      <th>reqProtocol:</th>
      <td>{{ reqProtocol }}</td>
    </tr>
    <tr>
      <th>reqHostname:</th>
      <td>{{ reqHostname }}</td>
    </tr>
    <tr>
      <th>reqPath:</th>
      <td>{{ reqPath }}</td>
    </tr>
    <tr>
      <th>reqMethod:</th>
      <td>{{ reqMethod }}</td>
    </tr>
  </table>
</div>
<div id="footer">
  {{ container }}
</div>
